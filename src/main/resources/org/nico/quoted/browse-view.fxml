<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane maxWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nico.quoted.ui.controller.BrowseViewController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView prefHeight="200.0" prefWidth="150.0">
              <columns>
                <TableColumn fx:id="sourceColumn" prefWidth="150.0" text="Source" />
              </columns>
            </TableView>
            <TableView prefHeight="400.0" prefWidth="450.0">
              <columns>
                <TableColumn fx:id="quoteColumn" prefWidth="400.0" text="Quote" />
                <TableColumn fx:id="editQuoteColumn" prefWidth="50.0" text="Edit" />
              </columns>
            </TableView>
         </children>
      </HBox>
   </children>
</AnchorPane>
