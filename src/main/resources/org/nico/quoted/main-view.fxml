<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="430.0" maxWidth="600.0" prefHeight="430.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nico.quoted.ui.controller.MainController">
    <TabPane fx:id="tabPane" prefHeight="430.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <Tab text="New">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <fx:include source="new-view.fxml"/>
            </AnchorPane>
        </Tab>
        <Tab text="Random">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <fx:include source="random-view.fxml"/>
            </AnchorPane>
        </Tab>
        <Tab text="Quotes">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <fx:include source="quotes-view.fxml"/>
            </AnchorPane>
        </Tab>
        <Tab text="Sources">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <fx:include source="source-table-view.fxml"/>
            </AnchorPane>
        </Tab>
    </TabPane>
</VBox>
